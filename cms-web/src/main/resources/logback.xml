<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <!-- 控制台日志输出设置 -->
    <appender name="CONSOLE-APPENDER" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss.SSS} [%green(%t)] %highlight(%-5p) [%boldMagenta(%logger{10})] - %m%n
            </pattern>
        </encoder>
    </appender>
    <root level="INFO">
        <appender-ref ref="CONSOLE-APPENDER"/>
    </root>
</configuration>
